n   state = {\n      modalOpen: false, \n      movieDetails: []\n   }\n   /* when \"more details\" button is pressed, fetch\n   from the api using the movie's id*/\n   handleButtonClick = () => {\n      // console.log(this.props.movie.id)\n      fetch(`https://imdb-internet-movie-database-unofficial.p.rapidapi.com/film/${this.props.movie.id}`, {\n         \"method\": \"GET\",\n         \"headers\": {\n            \"x-rapidapi-key\": process.env.REACT_APP_API_KEY,\n            \"x-rapidapi-host\": \"imdb-internet-movie-database-unofficial.p.rapidapi.com\",\n            \"useQueryString\": true\n         }\n      })\n         .then(response => response.json()) \n         // .then(data => console.log(data))\n         .then(data => this.setState({\n            modalOpen: true, \n            movieDetails: data\n         }))\n         .then(() => console.log(this.state.movieDetails))\n   }\n\n   render() {\n      return (\n         <div className=\"movie-card\">\n            <div className=\"movie-title-div\">\n               <span className=\"movie-title\">{this.props.movie.title}</span>\n            </div>\n            <div className=\"img-container\">\n               <img\n                  className=\"movie-image\"\n                  src={this.props.movie.image}\n                  alt={this.props.movie.title}\n               />\n            </div>\n            <button className=\"button\" onClick={this.handleButtonClick}>See Details</button>\n\n            <Modal\n               className=\"modal\"\n               show={this.state.modalOpen}\n               onHide={() => this.setState({\n                  modalOpen: false\n               })}\n            >\n               <div className=\"modal-div\">\n                  <Modal.Header closeButton>\n                     <Modal.Title>{this.state.movieDetails.title}</Modal.Title>\n                  </Modal.Header>\n\n                  <Modal.Body>\n                     <p>{this.state.movieDetails.year}</p>\n                     <p>{this.state.movieDetails.length}</p>\n                     <div className=\"img-container\">\n                        <img className=\"movie-image\" src={this.state.movieDetails.poster}></img>\n                     </div>\n                     <p>{this.state.movieDetails.plot}</p>\n                     <p>IMDB Rating: {this.state.movieDetails.rating}</p>\n                  </Modal.Body>\n\n                  <Modal.Footer>\n                     <button className=\"button\">Like</button>\n                     <button className=\"button\">Dislike</button>\n                  </Modal.Footer>\n\n               </div>\n            </Modal>\n         </div>\n      )\n   }\n}\n\nexport default MovieCard; ",{"ruleId":"25","severity":1,"message":"26","line":2,"column":8,"nodeType":"27","messageId":"28","endLine":2,"endColumn":19},{"ruleId":"29","severity":1,"message":"30","line":63,"column":25,"nodeType":"31","endLine":63,"endColumn":91},"no-unused-vars","'ModalHeader' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]